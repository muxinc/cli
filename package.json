{
  "name": "@mux/cli",
  "description": "Your friendly neighborhood Mux CLI tool!",
  "version": "0.6.0",
  "author": {
    "name": "Mux",
    "email": "devex@mux.com",
    "url": "https://docs.mux.com"
  },
  "contributors": [
    "Matt McClure <matt@mux.com>",
    "Ed Ropple <ed@mux.com>"
  ],
  "bin": {
    "mux": "./bin/run"
  },
  "bugs": "https://github.com/muxinc/cli/issues",
  "dependencies": {
    "@mux/mux-node": "^3.1.0",
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-autocomplete": "^0.2.0",
    "@oclif/plugin-commands": "^1.3.0",
    "@oclif/plugin-help": "^3.2.0",
    "@oclif/plugin-not-found": "^1.2.4",
    "@oclif/plugin-plugins": "^1.9.4",
    "@oclif/plugin-update": "^1.3.10",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "chalk": "^4.1.0",
    "clipboardy": "^2.3.0",
    "dotenv": "^8.2.0",
    "fs-extra": "^9.0.1",
    "inquirer": "^7.3.3",
    "listr": "^0.14.3",
    "request": "^2.88.2",
    "runtypes": "^5.0.1",
    "rxjs": "^6.6.3",
    "serialize-error": "^7.0.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.24.3",
    "@oclif/test": "^1.2.8",
    "@oclif/tslint": "^3",
    "@types/chai": "^4.2.14",
    "@types/fs-extra": "^9.0.4",
    "@types/inquirer": "^7.3.1",
    "@types/listr": "^0.14.2",
    "@types/mocha": "^8.0.4",
    "@types/node": "^10",
    "@types/request": "^2.48.5",
    "chai": "^4",
    "globby": "^11.0.1",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "oclif": "^1.16.1",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/muxinc/cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "mux",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-commands",
      "@oclif/plugin-update",
      "@oclif/plugin-warn-if-update-available",
      "@oclif/plugin-plugins"
    ],
    "macos": {
      "identifier": "com.mux.cli"
    },
    "topics": {
      "assets": {
        "description": "tools for managing Mux assets"
      },
      "profiles": {
        "description": "tools for managing profiles and user credentials"
      },
      "live": {
        "description": "tools for managing Mux live streams"
      },
      "plugins": {
        "description": "tools for managing plugins for the Mux CLI"
      },
      "signed-assets": {
        "description": "tools for working with signed assets (for private playback)"
      }
    }
  },
  "repository": "muxinc/cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "volta": {
    "node": "8.17.0"
  }
}
