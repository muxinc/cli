{
  "name": "@mux/cli",
  "description": "Your friendly neighborhood Mux CLI tool!",
  "version": "0.8.0",
  "author": {
    "name": "Mux",
    "email": "devex@mux.com",
    "url": "https://docs.mux.com"
  },
  "contributors": [
    "Matt McClure <matt@mux.com>",
    "Ed Ropple <ed@mux.com>"
  ],
  "bin": {
    "mux": "./bin/run"
  },
  "bugs": "https://github.com/muxinc/cli/issues",
  "dependencies": {
    "@mux/mux-node": "^4.0.0",
    "@oclif/command": "^1.8.16",
    "@oclif/config": "^1.18.2",
    "@oclif/plugin-autocomplete": "^1.1.1",
    "@oclif/plugin-commands": "^2.0.2",
    "@oclif/plugin-help": "^5.1.10",
    "@oclif/plugin-not-found": "^2.2.4",
    "@oclif/plugin-plugins": "^2.0.12",
    "@oclif/plugin-warn-if-update-available": "^2.0.4",
    "chalk": "4.1.2",
    "clipboardy": "^2.3.0",
    "dotenv": "^8.2.0",
    "fs-extra": "^10.0.0",
    "inquirer": "^8.2.0",
    "jsonwebtoken": "^8.5.1",
    "listr": "^0.14.3",
    "pkg": "^5.8.0",
    "request": "^2.88.2",
    "runtypes": "^6.5.0",
    "rxjs": "^7.5.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.10",
    "@oclif/test": "^2.0.3",
    "@oclif/tslint": "^3",
    "@types/chai": "^4.3.0",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.1.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/listr": "^0.14.4",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.12",
    "@types/request": "^2.48.8",
    "chai": "^4.3.6",
    "globby": "11.0.4",
    "mocha": "6.2.3",
    "nyc": "^15.1.0",
    "oclif": "^2.4.0",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/src",
    "/yarn.lock",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/muxinc/cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "mux",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-commands",
      "@oclif/plugin-warn-if-update-available",
      "@oclif/plugin-plugins"
    ],
    "macos": {
      "identifier": "com.mux.cli"
    },
    "topics": {
      "assets": {
        "description": "tools for managing Mux assets"
      },
      "live": {
        "description": "tools for managing Mux live streams"
      }
    }
  },
  "repository": "muxinc/cli",
  "scripts": {
    "clean": "rm -rf dist && rm -rf lib && rm -rf tsconfig.tsbuildinfo",
    "build": "tsc -b",
    "pkg": "rm -rf dist/* && pkg . --compress GZip",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "yarn run -s clean && yarn run -s build && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "volta": {
    "node": "14.16.1"
  },
  "pkg": {
    "scripts": "src/**/*.js",
    "assets": "package.json,lib/**/*.js",
    "targets": ["node18-linux-x64","node18-macos-arm64","node18-macos-x64","node18-macos-arm64","node18-windows-x64"],
    "output": "mux-cli",
    "outputPath": "dist"
  }
}
