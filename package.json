{
  "name": "mux-cli",
  "description": "Your friendly neighborhood Mux CLI tool!",
  "version": "0.0.0",
  "author": "Matthew McClure @mmcc",
  "bin": {
    "mux": "./bin/run"
  },
  "bugs": "https://github.com/muxinc/cli/issues",
  "dependencies": {
    "@mux/mux-node": "^2.2.0",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@types/listr": "^0.13.0",
    "chalk": "^2.4.2",
    "clipboardy": "^1.2.3",
    "inquirer": "^6.2.1",
    "listr": "^0.14.3",
    "request": "^2.88.0",
    "rxjs": "^6.3.3",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@oclif/tslint": "^3",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "@types/request": "^2.48.1",
    "chai": "^4",
    "globby": "^8",
    "mocha": "^5",
    "nyc": "^13",
    "ts-node": "^7",
    "tslint": "^5",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/muxinc/cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "mux",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "macos": {
      "identifier": "com.mux.cli"
    },
    "topics": {
      "assets": {
        "description": "tools for managing Mux assets"
      }
    }
  },
  "repository": "muxinc/cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
